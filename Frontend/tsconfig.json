{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "jsx": "react-jsx"
    },
  "include": [
    "src/**/*.d.ts",
    "src/**/*.ts",
    "src/**/*.astro",
    "src/**/*.d.tsx",
    "src/**/*.tsx", "src/components/Ticket.jsx",
  ]
}